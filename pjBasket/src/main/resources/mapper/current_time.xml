<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper     PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"     "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.mybatis.sql">
	<resultMap id="pMap" type="java.util.HashMap"/>
	<select id="current_time" parameterType="map" resultType="string">
		SELECT to_char(sysdate, 'YYYY-MM-DD HH24:MI:SS')
		FROM dual
	</select>
	<!-- procedure -->
	<update id="procedure" parameterType="map" statementType="CALLABLE">
		{call pt(#{param}, #{pc, jdbcType=CURSOR, mode=OUT,
		javaType=java.sql.ResultSet, resultMap=pMap})}
	</update>
</mapper>